"#playerWin": {
	fullscreen: false,
	backgroundColor:"#fff",
	layout: 'vertical',
	navBarHidden: true,
	exitOnClose: true
}

"#header": {
	height: "70dp",
	width: Ti.UI.FILL,
}

"#playerField":{
	width: '80%',
	left: 0,
	color:'#000',
	borderStyle: Ti.UI.INPUT_BORDERSTYLE_ROUNDED,
	returnKeyType: Ti.UI.RETURNKEY_DONE
}

"#addButton":{
	width: '20%',
	right: 0
}

"#playerTable": {
	top: Alloy.Globals.tableTop,
}

"#back": {
	top: Alloy.Globals.top,
	height: "70dp"
}